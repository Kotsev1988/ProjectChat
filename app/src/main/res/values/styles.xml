<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>







    <style name="MyCheckBox" parent="Theme.AppCompat.NoActionBar">
        <item name="colorControlNormal">#FFFAEA</item>
        <item name="colorControlActivated">#000</item>
        <item name="android:textColor">#FFFF3F3C</item>
    </style>

    <style name="Widget.AppTheme.Dim" parent="">
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="Widget.AppTheme.ActionBar.Fullscreen" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>


</resources>
